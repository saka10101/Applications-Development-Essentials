{
  "expo": {
    "name": "Bookmin",
    "slug": "bookmin-mobile",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.saka.bookmin",
      "buildNumber":"0.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.saka.bookmin",
      "versionCode": 1,
      "permissions" : ["WRITE_EXTERNAL_STORAGE"]
    }
  }
} 
